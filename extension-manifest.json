{
	manifestVersion: 1,
	id: "christaylor-buildandreleasetasks",
	name: "Build and Release Management Extensions by Chris Taylor (Polaris Solutions, LLC)",
	version: "0.1.0",
	publisher: "christaylor",
	public: false,
	targets: [
		{
			id: "Microsoft.VisualStudio.Services"
		}
	],
	description: "Build and Release Management Extensions",
	categories: [
		"Build and release"
	],
	tags: [
		"",
		"build",
		"webdeploy",
		"tokens",
		"docker"
	],
	content: {
		details: {
			path: "extension-overview.md"
		},
		license: {
			path: "license.txt"
		}
	},
	links: {
		home: {
			uri: "http://yakshaver.blogspot.com"
		},
		repository: {
			uri: "https://github.com/cege7480/vso-build-release-tasks"
		},
		issues: {
			uri: "https://github.com/cege7480/vso-build-release-tasks/issues"
		}
	},
	branding: {
		color: "rgb(36, 43, 50)",
		theme: "dark"
	},
	icons: {
		default: "images/extension-icon.png"
	},
	files: [
		{
			path: "Tasks/SwapAzureDeployment"
		}
	],
	contributions: [
		{
			id: "azure-swapcloudservicedeploymentslots-task",
			type: "ms.vss-distributed-task.task",
			targets: [
				"ms.vss-distributed-task.tasks"
			],
			properties: {
				name: "Tasks/SwapAzureDeployment"
			}
		}
	]
}